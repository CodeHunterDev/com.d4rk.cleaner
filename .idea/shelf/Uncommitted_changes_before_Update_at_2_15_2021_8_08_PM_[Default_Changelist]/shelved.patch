<+>package com.d4rk.cleaner;\rimport android.annotation.SuppressLint;\rimport android.content.pm.ApplicationInfo;\rimport android.content.pm.PackageManager;\rimport android.content.res.Resources;\rimport android.graphics.Color;\rimport android.widget.TextView;\rimport com.fxn.stash.Stash;\rimport java.io.File;\rimport java.util.ArrayList;\rimport java.util.Arrays;\rimport java.util.LinkedList;\rimport java.util.List;\rimport java.util.Locale;\rimport java.util.Objects;\rimport static com.d4rk.cleaner.WhitelistActivity.getWhiteList;\r@SuppressWarnings(\"StatementWithEmptyBody\")\rpublic class FileScanner {\r    private final File path;\r    private Resources res;\r    private MainActivity gui;\r    private int filesRemoved = 0;\r    private long kilobytesTotal = 0;\r    private boolean delete = false;\r    private boolean emptyDir = false;\r    private boolean autoWhite = true;\r    private boolean corpse = false;\r    private static final ArrayList<String> filters = new ArrayList<>();\r    private static final String[] protectedFileList = {\r            \"backup\", \"copy\", \"copies\", \"important\", \"do_not_edit\"};\r    FileScanner(File path) {\r        this.path = path;\r    }\r    private List<File> getListFiles() {\r        return getListFiles(path);\r    }\r    /**\r     * Used to generate a list of all files on device\r     * @param parentDirectory where to start searching from\r     * @return List of all files on device (besides whitelisted ones)\r     */\r    private synchronized List<File> getListFiles(File parentDirectory) {\r        ArrayList<File> inFiles = new ArrayList<>();\r        File[] files = parentDirectory.listFiles();\r        if (files != null) {\r            for (File file : files) {\r                if (!isWhiteListed(file)) { // won't touch if whitelisted\r                    if (file.isDirectory()) { // folder\r                        if (autoWhite) {\r                            if (!autoWhiteList(file))\r                                inFiles.add(file);\r                        }\r                        else inFiles.add(file); // add folder itself\r                        inFiles.addAll(getListFiles(file)); // add contents to returned list\r                    } else inFiles.add(file); // add file\r                }\r            }\r        }\r        return inFiles;\r    }\r\r    /**\r     * Runs a for each loop through the white list, and compares the path of the file\r     * to each path in the list\r     * @param file file to check if in the whitelist\r     * @return true if is the file is in the white list, false if not\r     */\r    private synchronized boolean isWhiteListed(File file) {\r        for (String path : getWhiteList())\r            if (path.equalsIgnoreCase(file.getAbsolutePath())\r                    || path.equalsIgnoreCase(file.getName()))\r                return true;\r        return false;\r    }\r    /**\r     * Runs before anything is filtered/cleaned. Automatically adds folders to the whitelist\r     * based on the name of the folder itself\r     * @param file file to check whether it should be added to the whitelist\r     */\r    private synchronized boolean autoWhiteList(File file) {\r\r        for (String protectedFile : protectedFileList) {\r            if (file.getName().toLowerCase().contains(protectedFile) &&\r                    !getWhiteList().contains(file.getAbsolutePath().toLowerCase())) {\r                getWhiteList().add(file.getAbsolutePath().toLowerCase());\r                Stash.put(\"whiteList\", getWhiteList());\r                return true;\r            }\r        }\r        return false;\r    }\r    /**\r     * Runs as for each loop through the filter, and checks if\r     * the file matches any filters\r     * @param file file to check\r     * @return true if the file's extension is in the filter, false otherwise\r     */\r    public synchronized boolean filter(File file) {\r        // corpse checking - TODO: needs improved!\r        if (file.getParentFile() != null && file.getParentFile().getParentFile() != null&& corpse)\r            if (file.getParentFile().getName().equals(\"data\") && file.getParentFile().getParentFile().getName().equals(\"Android\"))\r                if (!getInstalledPackages().contains(file.getName()) && !file.getName().equals(\".nomedia\"))\r                    return true;\r\r        if (file.isDirectory() && isDirectoryEmpty(file)\r                && emptyDir) return true; // empty folder\r\r        for (String filter : filters)\r            if (file.getAbsolutePath().toLowerCase()\r                    .matches(filter.toLowerCase()))\r                return true; // file\r\r        return false; // not empty folder or file in filter\r    }\r    private synchronized List<String> getInstalledPackages() {\r        final PackageManager pm = gui.getPackageManager();\r        @SuppressLint(\"QueryPermissionsNeeded\") List<ApplicationInfo> packages = pm.getInstalledApplications(PackageManager.GET_META_DATA);\r\r        List<String> packagesString = new LinkedList<>();\r        for (ApplicationInfo packageInfo : packages) {\r            packagesString.add(packageInfo.packageName);\r        }\r        return packagesString;\r    }\r    /**\r     * lists the contents of the file to an array, if the array length is 0, then return true,\r     * else false\r     * @param directory directory to test\r     * @return true if empty, false if containing a file(s)\r     */\r    private synchronized boolean isDirectoryEmpty(File directory) {\r\r        if (directory.list() != null) return Objects.requireNonNull(directory.list()).length == 0;\r        else return false;\r    }\r    /**\r     * Adds paths to the white list that are not to be cleaned. As well as adds\r     * extensions to filter. 'generic', 'aggressive', and 'apk' should be assigned\r     * by calling preferences.getBoolean()\r     */\r    @SuppressLint(\"ResourceType\")\r    synchronized void setUpFilters(boolean generic, boolean aggressive, boolean apk) {\r        List<String> folders = new ArrayList<>();\r        List<String> files = new ArrayList<>();\r        if (gui != null)\r            setResources(gui.getResources());\r        if (generic) {\r            folders.addAll(Arrays.asList(res.getStringArray(R.array.generic_filter_folders)));\r            files.addAll(Arrays.asList(res.getStringArray(R.array.generic_filter_files)));\r        }\r        if (aggressive) {\r            folders.addAll(Arrays.asList(res.getStringArray(R.array.aggressive_filter_folders)));\r            files.addAll(Arrays.asList(res.getStringArray(R.array.aggressive_filter_files)));\r        }\r        // filters\r        filters.clear();\r        for (String folder : folders)\r            filters.add(getRegexForFolder(folder));\r        for (String file : files)\r            filters.add(getRegexForFile(file));\r        // apk\r        if (apk) filters.add(getRegexForFile(\".apk\"));\r    }\r    @SuppressLint(\"SetTextI18n\")\r    long startScan() {\r        byte cycles = 0;\r        byte maxCycles = 10;\r        List<File> foundFiles;\r        if (!delete) maxCycles = 1; // when nothing is being deleted. Stops duplicates from being found\r        // removes the need to 'clean' multiple times to get everything\r        while (cycles < maxCycles) {\r            // find files\r            foundFiles = getListFiles();\r            if (gui != null) gui.scanPBar.setMax(gui.scanPBar.getMax() + foundFiles.size());\r            // scan & delete\r            for (File file : foundFiles) {\r                if (filter(file)) { // filter\r                    TextView tv = null;\r                    if (gui != null)\r                        tv = gui.displayPath(file);\r\r                    if (delete) {\r                        kilobytesTotal += file.length();\r                        ++filesRemoved;\r                        if (file.delete()) { // deletion\r\r                        } else if (tv != null) tv.setTextColor(Color.G