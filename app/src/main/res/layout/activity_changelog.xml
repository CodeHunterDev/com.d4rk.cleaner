<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginStart="5dp"
    android:layout_marginEnd="5dp"
    android:layout_marginTop="5dp"
    android:layout_marginBottom="5dp"
    app:cardCornerRadius="10dp"
    app:cardElevation="5dp"
    app:cardUseCompatPadding="true"
    android:id="@+id/layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.appcompat.widget.AppCompatEditText
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="100dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@color/colorPrimary"
        android:editable="false"
        android:focusable="false"
        android:gravity="center_horizontal"
        android:text="@string/changelog"
        android:textIsSelectable="false"
        tools:ignore="Deprecated" />

    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="bottom|center"
        android:src="@drawable/ic_d4rk"
        tools:ignore="ContentDescription" />
    <ImageView
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:rotation="@android:integer/config_longAnimTime"
        android:src="@drawable/ic_square_outline"
        android:layout_marginStart="6dp"
        android:layout_marginTop="10dp"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_square_outline"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="300dp"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_square_outline"
        android:layout_marginStart="60dp"
        android:layout_gravity="bottom"
        android:layout_marginBottom="-80dp"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="end"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_circle"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="6dp"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_gravity="top"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_circle"
        android:layout_marginStart="20dp"
        android:layout_marginTop="630dp"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="end"
        android:layout_marginEnd="-25dp"
        android:layout_marginTop="199dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_triangle"
        tools:ignore="ContentDescription" />
    <ImageView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_gravity="start|left"
        android:src="@drawable/ic_triangle"
        android:layout_marginLeft="-40dp"
        android:layout_marginTop="500dp"
        tools:ignore="ContentDescription,RtlHardcoded"/>
    <ImageView
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_gravity="end"
        android:layout_marginEnd="-70dp"
        android:layout_marginTop="333dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_square"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_gravity="end"
        android:layout_marginEnd="-99dp"
        android:layout_marginTop="440dp"
        android:src="@drawable/ic_roundline"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="69dp"
        android:layout_height="69dp"
        android:layout_marginTop="60dp"
        android:layout_marginStart="0dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_triangle_blue"
        tools:ignore="ContentDescription"/>
    <ImageView
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:layout_gravity="bottom|right|end"
        android:layout_marginRight="-60dp"
        android:layout_marginBottom="-69dp"
        android:rotation="@android:integer/status_bar_notification_info_maxnum"
        android:src="@drawable/ic_triangle_blue"
        tools:ignore="ContentDescription,RtlHardcoded"/>
</com.google.android.material.card.MaterialCardView>